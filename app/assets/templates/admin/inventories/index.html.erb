      <div class="text-center">
        <button class="btn btn-warning" ng-click="" ng-hide="noMoreResults"><i class="fa fa-search-plus" aria-hidden="true"></i> {{ 'display_more_items' | translate }}</button>
      </div>
      <p ng-if="items.length == 0" translate>{{ 'no_items_for_now' }}</p>
    </div>
