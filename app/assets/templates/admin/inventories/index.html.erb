            <th style="width:10%">
		<a class="ng-binding" href="" ng-click="">
			{{ 'maker' | translate }}
                </a>
	    </th>
            <th style="width:10%">
		<a class="ng-binding" href="" ng-click="">
			{{ 'date_of_purchase' | translate }}
                </a>
	    </th>
	    <th style="width:10%">
                <a class="ng-binding" href="" ng-click="">
                        {{ 'date_of_registration' | translate }}
                </a>
            </th>
          </tr>
        </thead>
